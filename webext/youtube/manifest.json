{
  "name": "play-pause",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"]
  },
  "commands": {
    "play-pause": {
      "suggested_key": {
        "default": "MediaPlayPause"
      }
    }
  },
  "content_scripts": [{
    "matches": ["https://www.youtube.com/*"],
    "js": ["play-pause.js"]
  }]
}
