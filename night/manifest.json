{
   "name": "Night",
   "version": "1.0.0",
   "manifest_version": 2,
   "background":
   {
      "scripts": ["background.js"]
   },
   "commands":
   {
      "windows":
      {
         "suggested_key": {"default": "MediaPlayPause"}
      },
      "mac":
      {
         "suggested_key": {"default": "F8"}
      }
   },
   "content_scripts": [{
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
   }]
}
